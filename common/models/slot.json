{
  "name": "slot",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "storeId": {
      "type": "string",
      "required": true
    },
    "startTime": {
      "type": "string",
      "required": true
    },
    "endTime": {
      "type": "string",
      "required": true
    },
    "maxPeoplePerSlot": {
      "type": "number",
      "required": true,
      "default": 15
    }, 
    "date": { 
      "type": "date", 
      "required": true 
    }
  },
  "validations": [],
  "relations": {
    "store": {
      "type": "belongsTo",
      "model": "store",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "bookings": {
      "type": "hasMany",
      "model": "booking",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
